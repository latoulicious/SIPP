{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,vue}\"",
    "prepare": "cd .. && husky web/.husky"
  },
  "dependencies": {
    "@vuestic/tailwind": "^0.1.3",
    "axios": "^1.6.7",
    "jwt-decode": "^4.0.0",
    "pdfmake": "^0.2.9",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuestic-ui": "^1.7.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "~1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "8.49.0",
    "eslint-plugin-vue": "9.17.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "material-design-icons-iconfont": "^6.7.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.28",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.0",
    "vite": "^4.5.1",
    "vitest": "^0.34.6",
    "vue-i18n": "^9.6.5",
    "vue-tsc": "^1.8.19"
  }
}
